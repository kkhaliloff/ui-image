{    
    "extends": "./tools/tsconfig",
    "compilerOptions": {
        "paths": {
            "@nativescript-community/ui-image": ["src/image"],
            "@nativescript-community/ui-image/*": ["src/image/*"],
            "@nativescript-community/ui-zoomimage": ["src/zoomimage"],
            "@nativescript-community/ui-zoomimage/*": ["src/zoomimage/*"],
            "@nativescript-community/ui-image-colorfilter": ["src/image-colorfilter"],
            "@nativescript-community/ui-image-colorfilter/*": ["src/image-colorfilter/*"],
       },
    }
}
